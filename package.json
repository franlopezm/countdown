    "lint": "eslint ./src",
    "lint:fix": "pnpm lint --fix",
    "preview": "vite preview",
    "prepare": "husky",
    "pre-commit": "pnpm lint:fix"
  },
  "dependencies": {
    "luxon": "^3.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-select": "^5.10.0",
    "react-timezone-select": "^3.2.8",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/luxon": "^3.4.2",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.1.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0"
    }
