{
  "name": "timers",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "dev": "vite",
    "dev:demo": "pnpm dev --mode demo",
    "build": "tsc -b && vite build",
